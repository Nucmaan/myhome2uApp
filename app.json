{
  "expo": {
    "name": "myhome2u App",
    "slug": "myhome2u-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myhome2u-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/myhome2u-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/myhome2u-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "myhome2u-app",
    "web": {
      "favicon": "./assets/myhome2u-favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
